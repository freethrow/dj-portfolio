{
  "name": "contentful-metalsmith-example",
  "version": "1.0.0",
  "description": "A example app that uses the contentful-metalsmith plugin",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful-labs/contentful-metalsmith-example"
  },
  "scripts": {
    "dev": "run-p watch serve",
    "serve": "live-server ./build",
    "start": "node build.js",
    "lint": "standard \"helpers/**/*.js\" \"build.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "gazer --pattern '{src,layouts,helpers,partials}/**/*' node build.js"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "contentful-metalsmith": "^0.11.0",
    "gazer": "^1.0.0",
    "glob": "^7.0.6",
    "handlebars": "^4.0.0",
    "lightbox2": "^2.11.3",
    "marked": "^1.2.7",
    "metalsmith": "^2.3.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-clean": "^0.0.0",
    "metalsmith-data-markdown": "0.0.3",
    "metalsmith-drafts": "^0.0.1",
    "metalsmith-layouts": "^1.8.1",
    "metalsmith-markdown": "^1.3.0",
    "metalsmith-sass": "^1.7.0",
    "minimatch": "^3.0.4",
    "node-sass": "4.14.1"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^1.2.0",
    "live-server": "^1.1.0",
    "npm-run-all": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
  "npm": ">=8.0.0",
  "node": ">=16.0.0"
},
}
